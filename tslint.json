{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "no-console": false,
    "arrow-parens": false,
    "variable-name": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "interface-name": false,
    "max-line-length": [
      true,
      120
    ]
  },
  "rulesDirectory": []
}
